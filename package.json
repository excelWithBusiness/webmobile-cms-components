{
  "name": "@excelwithbusiness/webmobile-cms-landing-pages",
  "version": "0.0.5",
  "description": "Shared CMS React components for Static GraphCms pages, used in Filtered products",
  "repository": {
    "type": "git",
    "url": "https://github.com/excelWithBusiness/webmobile-cms-landing-pages.git"
  },
  "license": "UNLICENSED",
  "author": "Filtered",
  "main": "dist/index.js",
  "sideEffects": false,
  "module": "dist/index",
  "types": "build/index",
  "files": [
    "/dist/**/*",
    "/public"
  ],
  "scripts": {
    "-- CUSTOM --": "",
    "prebuild": "cross-env BABEL_ENV=build run-p test -- -u typecheck",
    "build": "yarn clean && cross-env BABEL_ENV=build NODE_ENV=production yarn build:babel",
    "build-storybook": "cross-env build-storybook -c config/storybook -o static",
    "build:babel": "babel src --out-dir dist --config-file ./babel.config.js --source-maps --extensions .ts,.tsx,.js --no-comments",
    "build:noPreBuild": "yarn clean && cross-env BABEL_ENV=build NODE_ENV=production yarn build:babel",
    "automate:build": "./scripts/build.sh",
    "bump": "npm version patch",
    "clean": "rimraf -rf ./dist/* && rimraf tsconfig.build.tsbuildinfo",
    "dev": "cross-env BABEL_ENV=build NODE_ENV=development run-p dev:declarations dev:babel",
    "dev:babel": "babel src --out-dir dist --config-file ./babel.config.js --source-maps --extensions .ts,.tsx --no-comments --watch",
    "dev:declarations": "tsc --project tsconfig.build.json --watch --preserveWatchOutput",
    "format": "eslint --fix \"src/**/*{.ts,.tsx,.js,.jsx}\"",
    "generate:dts": "tsc --project tsconfig.build.json",
    "lint": "run-p lint:ts",
    "lint:css": "stylelint 'src/**/*.{js,jsx,ts,tsx}' --config stylelintrc.json",
    "lint:ts": "eslint \"src/**/*.{js,ts,tsx}\"",
    "release": "./scripts/release.sh",
    "build-and-release": "yarn automate:build && yarn release",
    "storybook": "cross-env BABEL_ENV=build NODE_ENV=development start-storybook --quiet -p 9001 -c config/storybook -s ./",
    "test": "jest --env=jsdom",
    "test:coverage": "yarn test --coverage",
    "test:watch": "jest --watch --runInBand",
    "testsuite": "yarn typecheck && yarn lint && yarn test",
    "testsuite:coverage": "yarn testsuite --coverage",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@excelwithbusiness/webmobile-sc-components": "^1.42.0",
    "@excelwithbusiness/webmobile-svg-library": "^0.0.10",
    "prismic-reactjs": "^1.3.3",
    "react": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.4",
    "@babel/parser": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@digitalroute/cz-conventional-changelog-for-jira": "^4.1.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/core-js": "^2.5.3",
    "@types/enzyme": "^3.10.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.1.3",
    "@types/prop-types": "^15.5.3",
    "@types/qs": "^6.5.3",
    "@types/react": "16.9.44",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "1.10.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "conventional-changelog-eslint": "^3.0.9",
    "cross-env": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "file-loader": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^25.1.0",
    "jest-each": "^25.1.0",
    "jest-styled-components": "^7.0.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^16.2.0",
    "lint-staged": "^10.2.10",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^1.19.1",
    "raf": "^3.4.0",
    "react-test-renderer": "16.13.1",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.0",
    "storybook-react-router": "^1.0.8",
    "styled-components": "^5.1.1",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "themeprovider-storybook": "^1.4.0",
    "ts-jest": "^25.2.1",
    "typescript": "3.9.7",
    "url-loader": "^3.0.0"
  },
  "peerDependencies": {
    "@excelwithbusiness/webmobile-sc-components": "^1.42.0",
    "@excelwithbusiness/webmobile-svg-library": "^0.0.10",
    "prismic-reactjs": "^1.3.3",
    "react": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "comment": "Commit properly yo!!",
      "skipScope": false,
      "skipReference": true,
      "path": "@digitalroute/cz-conventional-changelog-for-jira"
    }
  }
}
